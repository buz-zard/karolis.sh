{
  "private": true,
  "repository": {
    "type": "git",
    "url": "git@github.com:karolis-sh/karolis.sh.git"
  },
  "license": "MIT",
  "author": "Karolis Å arapnickis",
  "scripts": {
    "build": "gatsby build",
    "check-deps": "npm ls >/dev/null",
    "clean": "rimraf public .cache",
    "format": "run-p format:*",
    "format:js": "prettier --write '**/*.*'",
    "format:package-json": "sort-package-json",
    "lint": "eslint src",
    "prepush": "npm run qa",
    "qa": "run-p lint",
    "start": "gatsby develop",
    "try": "run-s clean build && gatsby serve",
    "upgrade-interactive": "npm-check --update"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "@emotion/core": "^10.0.17",
    "@emotion/styled": "^10.0.17",
    "gatsby": "^2.15.20",
    "gatsby-image": "^2.2.20",
    "gatsby-plugin-advanced-sitemap": "^1.4.4",
    "gatsby-plugin-emotion": "^4.1.7",
    "gatsby-plugin-google-analytics": "^2.1.17",
    "gatsby-plugin-layout": "^1.1.8",
    "gatsby-plugin-preact": "^3.1.11",
    "gatsby-plugin-react-helmet": "^3.1.8",
    "gatsby-plugin-remove-serviceworker": "^1.0.0",
    "gatsby-plugin-robots-txt": "^1.5.0",
    "gatsby-plugin-s3": "^0.3.2",
    "npm-check": "^5.9.0",
    "preact": "^10.0.0-rc.3",
    "prop-types": "^15.7.2",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-helmet": "^5.2.1",
    "react-inlinesvg": "^1.1.6"
  },
  "devDependencies": {
    "@k.sh/eslint-config": "^1.2.0",
    "cz-conventional-changelog": "^2.1.0",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^17.0.0",
    "eslint-config-prettier": "^2.10.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-prettier": "^2.6.2",
    "eslint-plugin-react": "^7.11.1",
    "husky": "^0.14.3",
    "npm-run-all": "^4.1.3",
    "prettier": "^1.14.2",
    "rimraf": "^2.6.2",
    "sort-package-json": "^1.15.0"
  }
}
