{
  "name": "karolis.sh",
  "version": "1.0.0",
  "description": "Personal website",
  "repository": "git@github.com:buz-zard/kode.lt.git",
  "author": "Karolis <pastas.k@gmail.com>",
  "license": "MIT",
  "config": {
    "test_url": "https://test.karolis.sh/",
    "prod_url": "https://karolis.sh/",
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "scripts": {
    "develop": "gatsby develop",
    "build": "gatsby build",
    "clean": "rimraf public .cache",
    "try-build": "rm -rf public && yarn build && gatsby serve",
    "prepush": "yarn qa",
    "qa": "run-p lint",
    "lint": "eslint src",
    "test-siteqa": "run-s test-siteqa:*",
    "test-siteqa:lighthouse": "lighthouse $npm_package_config_test_url --output-path=./test.lightouse.report.html && open test.lightouse.report.html",
    "test-siteqa:sonarwhal": "sonarwhal $npm_package_config_test_url",
    "prod-siteqa": "run-s prod-siteqa:*",
    "prod-siteqa:lighthouse": "lighthouse $npm_package_config_prod_url --output-path=./prod.lightouse.report.html && open prod.lightouse.report.html",
    "prod-siteqa:sonarwhal": "sonarwhal $npm_package_config_prod_url"
  },
  "dependencies": {
    "babel-plugin-emotion": "^9.2.5",
    "emotion": "^9.2.5",
    "emotion-server": "^9.2.5",
    "gatsby": "^1.9.273",
    "gatsby-image": "^1.0.54",
    "gatsby-plugin-emotion": "^1.1.17",
    "gatsby-plugin-google-analytics": "^1.0.31",
    "gatsby-plugin-offline": "^1.0.18",
    "gatsby-plugin-preact": "^1.0.17",
    "gatsby-plugin-react-helmet": "^2.0.11",
    "gatsby-plugin-robots-txt": "^1.1.0",
    "gatsby-plugin-sitemap": "^1.2.25",
    "react-emotion": "^9.2.5",
    "react-helmet": "^5.2.0",
    "react-inlinesvg": "^0.8.1"
  },
  "devDependencies": {
    "@k.sh/eslint-config": "1.1.0",
    "@sonarwhal/formatter-stylish": "^2.0.4",
    "@sonarwhal/rule-apple-touch-icons": "^3.0.2",
    "@sonarwhal/rule-axe": "^4.0.2",
    "@sonarwhal/rule-content-type": "^3.0.4",
    "@sonarwhal/rule-disown-opener": "^3.0.5",
    "@sonarwhal/rule-highest-available-document-mode": "^3.0.4",
    "@sonarwhal/rule-html-checker": "^4.0.0",
    "@sonarwhal/rule-manifest-app-name": "^4.0.2",
    "@sonarwhal/rule-manifest-exists": "^4.0.1",
    "@sonarwhal/rule-manifest-file-extension": "^4.0.1",
    "@sonarwhal/rule-manifest-is-valid": "^5.1.0",
    "@sonarwhal/rule-meta-charset-utf-8": "^3.0.3",
    "@sonarwhal/rule-meta-viewport": "^3.0.2",
    "@sonarwhal/rule-no-disallowed-headers": "^3.0.4",
    "@sonarwhal/rule-no-friendly-error-pages": "^3.0.2",
    "@sonarwhal/rule-no-html-only-headers": "^3.0.5",
    "@sonarwhal/rule-no-http-redirects": "^3.0.3",
    "@sonarwhal/rule-no-protocol-relative-urls": "^3.0.2",
    "@sonarwhal/rule-no-vulnerable-javascript-libraries": "^3.3.0",
    "@sonarwhal/rule-strict-transport-security": "^3.0.2",
    "@sonarwhal/rule-validate-set-cookie-header": "^3.0.2",
    "@sonarwhal/rule-x-content-type-options": "^3.0.3",
    "cz-conventional-changelog": "^2.1.0",
    "eslint": "^5.1.0",
    "eslint-config-airbnb": "17.0.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-prettier": "^2.6.2",
    "eslint-plugin-react": "^7.9.1",
    "husky": "^0.14.3",
    "lighthouse": "^3.0.1",
    "npm-run-all": "^4.1.3",
    "prettier": "^1.13.7",
    "rimraf": "^2.6.2",
    "sonarwhal": "^1.11.2"
  }
}
